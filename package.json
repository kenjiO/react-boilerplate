{
  "name": "redux-react-starter",
  "version": "1.2.0",
  "description": "Get started.",
  "scripts": {
    "start": "webpack-dev-server --config webpack/webpack.config.dev.js",
    "build": "rm -rf dist && webpack --config webpack/webpack.config.prod.js  && cp -a assets/ dist/",
    "prod": "serve ./dist -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/didierfranc/redux-react-starter.git"
  },
  "author": "Didier Franc",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-styled-components": "0.0.3-0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.22.0",
    "html-webpack-plugin": "^2.24.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "react": "^15.4.2",
    "react-code-splitting": "^1.0.3",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.2",
    "react-router-dom": "4.0.0-beta.3",
    "redux": "^3.5.2",
    "redux-thunk": "^2.2.0",
    "styled-components": "^1.4.2"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "react",
      "stage-0"
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "helpers": false,
          "polyfill": false
        }
      ]
    ],
    "env": {
      "development": {
        "plugins": [
          "styled-components"
        ]
      },
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  }
}
